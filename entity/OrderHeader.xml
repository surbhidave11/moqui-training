<?xml version="1.0" encoding="UTF-8"?>

<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">

    <entity entity-name="OrderHeader" package="moqui.training">
        <field name="orderId" type="text-medium" is-pk="true" />
        <field name="orderDate" type="date" not-null="true"/>
        <field name="firstName" type="text-medium" />
        <field name="lastName" type="text-medium"/>

        <relationship type="one" related="Party" fk-name="FK_Order_Party">
            <key-map field-name="partyId" related="partyId"/>
        </relationship>
        <relationship type="one" related="ContactMech" fk-name="FK_Order_ShippingContactMech">
            <key-map field-name="shippingContactMechId" related="contactMechId"/>
        </relationship>
        <relationship type="one" related="ContactMech" fk-name="FK_Order_BillingContactMech">
            <key-map field-name="billingContactMechId" related="contactMechId"/>
        </relationship>
    </entity>


</entities>

